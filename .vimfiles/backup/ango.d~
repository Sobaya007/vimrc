import std.stdio, std.string,std.range, std.conv, std.array, std.algorithm, std.math, std.typecons;
import std.file;
import std.bigint;

void main() {

	string text = readText("ango.txt");
	text = "ff:ff";
	auto strs = text.split(":");
	BigInt num = BigInt(0);
	foreach (s; strs) {
		num *= 0xff;
		auto x = 0;
		if ('a' <= s[0] && s[0] <= 'f') x += 10 + s[0] - 'a';
		else x += s[0] - '0';
		x *= 16;
		if ('a' <= s[1] && s[1] <= 'f') x += 10 + s[1] - 'a';
		else x += s[1] - '0';
		num += x;
	}

	num = BigInt(text.replace(":", ""));
	writeln(num);
}

