import core.thread;
import sbylib;
import enemy;
import model;
import collision;
import scene;
import Crystal;
import WoodFrame;
import player;
static import std.file;

void main()
{
  if (!gameMain()) {
    while (true) {}
  }
}

bool gameMain() {
  //====================初期化==========================
  SbyWorld.init(17); //sbylib初期化
  auto window = SbyWorld.currentWindow;

  SceneManager sceneManager = SceneManager(SceneGame());
  EntityManager.getInstance.add(sceneManager);
  SbyWorld.mainLoop;

  return true;
}
