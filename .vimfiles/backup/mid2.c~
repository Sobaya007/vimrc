  j50360
  柿沼　建太郎 */

#include <stdio.h>

//メモ用配列
int memo[17][17];

//セル
char cell[17][17];

//(x,y)に至るまでの最短経路の個数
int dp(int x, int y) {
  if (memo[x][y] >= 0) return memo[x][y];
  if (x == 0 && y == 0) return 1;
  if (cell[x][y] == '*') return 0;
  int result = 0;
  if (0 < x) result += dp(x-1,y);
  if (0 < y) result += dp(x, y-1);
  return memo[x][y] = result;
}

int main() {

  //ファイル読み込み
  FILE *fp;
  fp = fopen("in.txt", "r");

  int H, W;
  fscanf(fp, "%d %d\n", &H, &W);
  
  int i, j;
  for (i = 0; i < H; i++) {
    for (j = 0; j < W; j++) {
      fscanf(fp, "%c", &cell[j][i]);
      if (j < W-1) fscanf(fp, " ");
    }
    fscanf(fp, "\n");
  }

  //メモ用配列を初期化
  for (i = 0; i < 17; i++) {
    for (j = 0; j < 17; j++) {
      memo[i][j] = -1;
    }
  }

  //結果を描画
  printf("%d\n", dp(W-1, H-1));

  return 0;
}
