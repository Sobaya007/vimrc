module model.modelloader.assetException;

import model.imports;

// ASSIMP関連例外
class AssetException : Exception {
	this(string msg, string file = __FILE__, size_t line = __LINE__, Throwable next = null) @nogc {
		super(msg, file, line, next);
	}
	this(string msg, Throwable next, string file = __FILE__, size_t line = __LINE__) @nogc {
		super(msg, file, line, next);
	}
}