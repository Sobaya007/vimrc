#include <stdio.h>

//セル
char cell[17][17];

int H, W;
int flags[17][17];

//(x,y)に至るまでの最短経路の個数とその回数を返す
int dp(int x, int y, int *rCount, int *rStep) {
  if (cell[x][y] == '*') return 0;
  if (x < 0) return 0;
  if (y < 0) return 0;
  if (x == W) return 0;
  if (y == H) return 0;
  if (flags[x][y] == 1) return 0;
  if (x == 0 && y == 0) {
    *rCount = 1;
    *rStep = 1;
    return 1;
  }
  flags[x][y] = 1;
  int count, step;
  if (dp(x-1,y,&count, &step)) {
    if (*rStep == -1) {
      *rStep = step;
      *rCount = count;
    } else if (*rStep > step) {
      *rStep = step;
      *rCount = count;
    } else if (*rStep == step) {
      *rCount += count;
    }
  }


  if (dp(x,y-1,&count, &step)) {
    if (*rStep == -1) {
      *rStep = step;
      *rCount = count;
    } else if (*rStep > step) {
      *rStep = step;
      *rCount = count;
    } else if (*rStep == step) {
      *rCount += count;
    }
  }
  flags[x][y] = 0;
  return 1;
}

int main() {

  //ファイル読み込み
  FILE *fp;
  fp = fopen("in.txt", "r");

  fscanf(fp, "%d %d\n", &H, &W);
  
  int i, j;
  for (i = 0; i < H; i++) {
    for (j = 0; j < W; j++) {
      fscanf(fp, "%c", &cell[j][i]);
      if (j < W-1) fscanf(fp, " ");
    }
    fscanf(fp, "\n");
  }

  for (i = 0; i < H; i++) {
    for (j = 0; j < W; j++) {
      flags[j][i] = 0;
    }
  }

  //結果を描画
  int count, step;
  count = step = 0;
  dp(W-1,H-1, &count, &step);
  printf("%d\n", count); 

  return 0;
}
